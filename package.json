{
  "name": "zine-skunk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node pdf.js --show",
    "dev": "next dev",
    "export": "node pdf.js",
    "test": "run-s -l test:*",
    "test:eslint": "eslint '{src,test}/**/*.{js,ts,tsx}' --ext .ts,.tsx",
    "test:prettier": "prettier '{src,test}/**/*.{css,js,ts,tsx}' -l",
    "test:ts": "tsc --noEmit",
    "test:stylelint": "stylelint 'src/**/*.{css,tsx}'",
    "prettier": "prettier --write '{src,test}/**/*.{css,js,ts,tsx}'"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.5.5",
    "@next/mdx": "^9.2.1",
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.1.6",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/source-map-support": "^0.5.0",
    "astroturf": "^0.10.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "classnames": "^2.2.6",
    "envalid": "^6.0.0",
    "next": "^9.3.2",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.0.3",
    "next-optimized-images": "^2.5.4",
    "puppeteer": "^2.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/puppeteer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylelint": "^13.0.0",
    "stylelint-config-lostfictions": "^0.9.1"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "module-resolver",
            {
              "root": [
                "."
              ],
              "alias": {
                "~": "./src"
              }
            }
          ]
        ]
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=12"
  }
}
